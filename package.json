{
    "name": "x-babyscript",
    "displayName": "X-series Babyscript",
    "description": "Syntax Highlighting for X-series Babyscript",
    "version": "0.0.1",
    "publisher": "JohnnyJingle",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "main",
    "activationEvents": [
        "onCommand:xbs.watch"
    ],
    "contributes": {
        "languages": [
            {
                "id": "xbabyscript",
                "aliases": [
                    "X-series Babyscript",
                    "xbabyscript"
                ],
                "extensions": [
                    ".xbs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "xbabyscript",
                "scopeName": "source.xrbs",
                "path": "./syntaxes/xrebirthbabyscript.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "xbs.watch",
                "title": "X-Babyscript: Watch"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^10.12.18",
        "typescript": "^3.2.2",
        "vscode": "^1.1.26"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    }
}
